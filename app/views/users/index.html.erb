<p id="notice"><%= notice %></p>

<h1>Pick a username</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <p><%= link_to user.username, users_path(user_id: user.id), method: :post %></p>
    <% end %>
  </tbody>
</table>

<br>
